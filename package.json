{
    "name": "bookshell",
    "version": "1.0.0",
    "description": "Serverless webpack example using ecma script",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "npm run lint && sls offline",
        "deploy": "npm run lint && sls deploy",
        "lint": "eslint .",
        "lint:updated": "pipe-git-updated --ext=js -- eslint --cache",
        "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
        "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\""
    },
    "dependencies": {
        "@koa/cors": "2.2.3",
        "aws-sdk": "2.562.0",
        "aws-serverless-koa": "1.0.1",
        "cross-fetch": "3.0.4",
        "isomorphic-fetch": "2.2.1",
        "koa": "2.11.0",
        "koa-bodyparser": "4.2.1",
        "koa-joi-router": "6.0.0",
        "koa-mount": "4.0.0",
        "koa-requestid": "2.0.1",
        "koa-router": "7.4.0",
        "koa-session": "5.12.3",
        "serverless-http": "^2.3.1",
        "uuid": "3.3.3"
    },
    "devDependencies": {
        "@serverless/eslint-config": "^1.2.1",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "git-list-updated": "^1.2.1",
        "prettier": "^1.19.1",
        "serverless-offline": "^12.0.3",
        "serverless-plugin-include-dependencies": "^4.0.1",
        "serverless-webpack": "^5.3.1",
        "webpack": "^4.35.2"
    },
    "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
    "license": "MIT"
}
